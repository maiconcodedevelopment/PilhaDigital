				try { dispatchEventListener('nodata', m ); } catch(exEvent){}
				addAdServerCounter(5550001577);
				notifyMetricsMonitor();
            } else if (e.data == 'dynad-tv-protocol/close') {
				try { dispatchEventListener('closed'); } catch(exEvent){}

