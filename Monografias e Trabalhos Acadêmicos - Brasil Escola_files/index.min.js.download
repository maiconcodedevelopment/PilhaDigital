function DropDown(a) {
    this.dd = a;
    this.placeholder = this.dd.children("span");
    this.opts = this.dd.find("ul.dropdown > li");
    this.val = "";
    this.index = -1;
    this.initEvents()
}
DropDown.prototype = {
    initEvents: function () {
        var a = this;
        a.dd.on("click", function (b) {
            $(this).toggleClass("active");
            return false
        });
        a.opts.on("click", function () {
            var b = $(this);
            a.val = b.text();
            a.index = b.index();
            a.placeholder.text(a.val)
        })
    },
    getValue: function () {
        return this.val
    },
    getIndex: function () {
        return this.index
    }
};
